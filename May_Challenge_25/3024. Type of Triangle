class Solution {
    public String triangleType(int[] nums) {
        int a = nums[0];
        int b = nums[1];
        int c = nums[2];

        // First check if the sides do not form a triangle
        if ((a + b <= c) || (a + c <= b) || (b + c <= a)) {
            return "none";
        }

        // Check for equilateral
        if (a == b && b == c && c==a) {
            return "equilateral";
        }

        // Check for scalene
        if (a != b && b != c && c!=a) {
            return "scalene";
        }

        // Else, it's isosceles
        return "isosceles";
    }
}
